/* pages/fc3dfilter/numb/numb.wxss */
@import '../../common/roundnooption.wxss';
.numberALL{
  margin:20rpx ;
  border:1rpx solid #e6e6e6;
}
.num_code {
   padding:20rpx 60rpx;
  display:flex;
  justify-content:space-around;
  height:60rpx;
  line-height:40rpx;
  font-size:28rpx;
  color: #4a4a4a;
  border-bottom:1rpx solid #e6e6e6;
}
.num_detail  {
  display:block;
  padding:10rpx 30rpx;
  border:4rpx;
  margin-right:20rpx;
  border-radius:6rpx;
  border:1rpx solid #e6e6e6;
}
.pic_group {
  width:100%;
  height:80rpx;
  display: flex;
  justify-content: center;
}
.pic_group image {
  width:200rpx;
  height:80rpx;
}
.text  {
  margin: 20rpx;
  width:auto;
  height:200rpx;
  border:1rpx solid #e6e6e6 ;
}
.content  {
   margin: 20rpx;
  width:auto;
  height:200rpx;
  border:1rpx solid #e6e6e6; 
  padding:20rpx;
  overflow:scroll;
  display: block;
}
.content  .se{
  width:100%;
  line-height:40rpx;
  display: flex;
}
.content  .main{
  margin-right: 20rpx;
  
}
.content  .clearText{
  width: 150rpx;
  
}


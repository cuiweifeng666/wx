<view class="number">
  <block wx:for="{{box}}" wx:for-index="a" >
      <view class="num_box{{box[a] == true?' number_choose_item_red':''}}" bindtap="boxTap" data-ch="{{a}}">{{a}}</view>
  </block>
</view>
<view class='btn'>
    <view class='cancel'>取消</view>
    <view class='sure'>确定</view>
</view>
<view class='select clearfix'>
  <text class='fl'>奇偶比例</text>
  <view class='select_name fl'>
     <view wx:for="{{select}}" wx:for-index="t" class="select_group" >
      {{item}}
      </view>
  </view>
</view>

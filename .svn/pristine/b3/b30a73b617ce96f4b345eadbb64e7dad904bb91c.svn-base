.number_box{ 
   padding:0 10rpx;
}
.fl {
  float:left;
}
.number_box_h1{
    line-height: 40rpx;
    color: #4a4a4a;
    padding:10rpx;
    font-size: .8rem;
    height:40rpx;
    font-weight: bold;
    background:none;
    padding-left: 30rpx;
    border-bottom:1rpx solid #e4e4e4;
}
.number_box_h1 span{
	display: inline-block;
  margin-top:5rpx;
    width: .1rem;
    height: .8rem;
    background:#da3231;
    margin-right: .4rem;
}
 .number_box_h1 navigator{
    padding-right: 1.4rem;
    float: right;
}
.diagShow {
  display: inline-block;
   background:none;
  width:40rpx;
  height:40rpx;
  margin-left:20rpx;
  
}
.number_box_context{	
  height:33px;
	line-height:33px;  
  display:-webkit-box;
  padding:20rpx 0 0 40rpx;
}
.number_box_context view view{
	display:inline-block;     
	width:60rpx;
	height:60rpx;
	line-height:60rpx;
	border-radius:60rpx;
	background:#fff;
	color:#b9b9b9;
	border:1px solid #b9b9b9;
	text-align:center;
	margin-right:10rpx;
}

.number_choose_item_red{
	color:#fff!important;
	border:1px solid #d70e07!important;
	background:#d70e07!important;
}
.red {
	color:#fff!important;
	background:#d70e07!important;
}
.number_box_control{	
  width:100%;
  height:60rpx;
  margin-top:20rpx;
  margin-bottom:20rpx;
  text-align:center;
}
.number_box_control button{
	width:5rem;
	height:1.5rem;
	line-height:1.5rem;
	border-radius:.3rem;
	color:#666;
	font-size:.8rem;
  display:inline-block;
  margin: 0 .25rem;
}
.number_box_footer{
  	line-height:23px;
	margin:.5rem 0;
  display:-webkit-box;
}

.number_box_footer text{
	color: #454545;
    font-size: .8rem;
    display: inline-block;
    width: 2.5rem;
}

.number_box_footer view view{
    display: inline-block;
    width: 1.2rem;
    height: 1.2rem;
    line-height: 1.2rem;
    border-radius: 3rem;
    background: #fff;
    color: #b9b9b9;
    border: 1px solid #b9b9b9;
    text-align: center;  
    margin-right:10rpx; 
}

.num_group{	
	line-height:60rpx;  
  height:80rpx;
  display:-webkit-box;
  padding:20rpx 0 0 20rpx;
  border-bottom:1px solid #e4e4e4;
}
.num_group .numLit {
	display:inline-block;     
	width:2.2rem;
	height:80rpx;
	line-height:60rpx;
	border-radius:3rem;
	color:#b9b9b9;
	text-align:center;
	margin-right:5rpx;
}
.num_group view view{
	display:inline-block;     
	width:50rpx;
	height:50rpx;
	line-height:50rpx;
	border-radius:92rpx;
	background:#fff;
	color:#b9b9b9;
	border:1px solid #b9b9b9;
	text-align:center;
	margin-right:10rpx;
}
.bottom {
  width:100%;
  padding: 20rpx 0;
  display: -webkit-box;
}
.bottom  button {
    display:inline-block;
    width:345rpx;
    margin-left:20rpx ;
    border-radius:5rpx;
    height:80rpx;
    font-size:32rpx;
    line-height:80rpx;
    color: #fff;
  }
.cancel  {
  background:#4A90E2 ;
}
.sure  {
  background: #da3231;
}
.number_choose_button{
	display:-webkit-box;
	-webkit-box-pack:center;
	/* margin-top:.5rem; */
  display:-moz-box;
  -moz-box-pack:center;
  text-align:center;
  width:100%;
  height:33px;
	line-height:33px;  
	padding:0 8px; 
  display:-webkit-box;
  margin:20rpx 0;
}
.number_choose_button_item{
	display:-webkit-box;
	margin:0 .5rem;
	height:1.5rem;
	line-height:1.5rem;
    display:-moz-box;
}
.number_choose_button_item image{
	width:1.5rem;
	height:1.5rem;
}
.number_choose_button_item view{
	width:2.5rem;
	height:1.4rem;
	line-height:1.4rem;
	background:#fff;
	font-size:.8rem;
	color:#b9b9b9;
	border:1px solid #b9b9b9;
	text-align:center;
	margin-left:-1px;
}
